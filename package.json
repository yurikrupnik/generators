{
  "name": "generators",
  "version": "1.0.0",
  "description": "Custom Yeoman generators monorepo",
  "scripts": {
    "lint:scoped": "lerna run lint --scope generator-node-puzzle-eslint",
    "dds": "lerna run build",
    "builds": "lerna run build --scope generator-node-puzzle-babel",
    "start": "todo create exit 1",
    "lint": "lerna exec --parallel -- eslint src",
    "pretests": "npm run lint",
    "test": "jest",
    "prebuild": "lerna exec -- rimraf app && npm run test",
    "prewatch": "npm run prebuild",
    "build": "lerna exec -- rollup -c=../../rollup.config.js",
    "prepublishOnly": "npm run build",
    "publish": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/generators.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/generators/issues"
  },
  "homepage": "https://github.com/yurikrupnik/generators#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.8.0",
    "lerna": "^3.14.1",
    "rollup": "^1.12.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ]
  }
}
